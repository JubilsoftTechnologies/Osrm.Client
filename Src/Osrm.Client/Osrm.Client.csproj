<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>3.5.0</Version>
    <Authors>Scott Morrison</Authors>
    <Company>Jubilsoft Technologies LLC</Company>
    <PackageId>Osrm.Client.NetCore</PackageId>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <Product>Osrm.Client.NetCore</Product>
    <Description>Http client for OSRM 5x API based on .Net Core</Description>
    <Copyright>Copyright (c) 2020 Jubilsoft Technologies, LLC</Copyright>
    <RepositoryUrl>https://github.com/jubilee-scott/Osrm.Client</RepositoryUrl>
    <PackageProjectUrl>https://github.com/jubilee-scott/Osrm.Client</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
